{
  "version": 2,
  "env": {
    "DEPLOY_TIME": "2024-01-10-1"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/index.js"
    },
    {
      "src": "/api/celebcatz/(.*)",
      "dest": "/api/celebcatz/index.js"
    },
    {
      "src": "/api/token-metrics",
      "dest": "/api/token-metrics.js"
    },
    {
      "src": "/api/top-holders",
      "dest": "/api/top-holders.js"
    },
    {
      "src": "/api/collections/([^/]+)/(.*)",
      "dest": "/api/collections/index.js?collection=$1&endpoint=$2"
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.buxdao.com"
        }
      ],
      "status": 301,
      "dest": "https://buxdao.com/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 