{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/token-metrics",
      "dest": "/api/token-metrics.js"
    },
    {
      "src": "/api/top-holders",
      "dest": "/api/top-holders.js"
    },
    {
      "src": "/api/collections/([^/]+)/stats",
      "dest": "/api/collections/[symbol]/stats.js"
    },
    {
      "src": "/api/celebcatz/images",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/printful/products/([^/]+)",
      "dest": "/api/printful/products/[id].js"
    },
    {
      "src": "/api/printful/products",
      "dest": "/api/printful/products.js"
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.buxdao.com"
        }
      ],
      "status": 301,
      "dest": "https://buxdao.com/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 