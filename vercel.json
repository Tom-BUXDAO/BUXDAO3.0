{
  "version": 2,
  "env": {
    "DEPLOY_TIME": "2024-03-21-1"
  },
  "crons": [
    {
      "path": "/api/rewards/process-daily",
      "schedule": "0 0 * * *"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/discord-interactions.js": {
      "runtime": "edge",
      "memory": 256,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/discord-interactions",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/discord-interactions"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
} 